{
  "name": "universal-react-relay-starter-kit",
  "version": "0.1.0",
  "description": "Create a React Relay app with server side rendering and code splitting",
  "main": "client/main.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "clean": "rimraf build buildClient buildClientSSR",
    "start": "npm run clean && npm run start:servers",
    "start:servers": "npm-run-all --parallel relay-compiler:watch start:app start:graphql",
    "start:app": "DEBUG=server* babel-node server/render",
    "start:graphql": "nodemon --watch server/graphql --exec \"DEBUG=graphql* babel-node server/graphql\"",
    "start:prod": "npm-run-all --parallel relay-compiler start:prod:app start:prod:graphql",
    "start:prod:app": "node build/server/render",
    "start:prod:graphql": "node build/server/graphql",
    "relay-compiler": "relay-compiler --src client --schema server/graphql/schema.graphql",
    "relay-compiler:watch": "relay-compiler --src client --schema server/graphql/schema.graphql --watch",
    "update-schema": "babel-node ./scripts/updateSchema.js",
    "build": "npm run clean && npm run build:common && npm run build:server && cp -R webpack build/webpack && cp -R client build/client",
    "build:common": "babel -d ./build/common/ ./common -s",
    "build:server": "babel -d ./build/server/ ./server -s",
    "build-langs": "babel-node ./i18n/bundleMessages.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jkettmann/universal-react-relay-starter-kit.git"
  },
  "keywords": [
    "starter-kit",
    "boilerplater",
    "graphql",
    "relay",
    "react",
    "reactjs",
    "ssr",
    "server-side-rendering",
    "universal",
    "isomorphic",
    "seo",
    "internationalization",
    "i18n"
  ],
  "author": "Johannes Kettmann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jkettmann/universal-react-relay-starter-kit/issues"
  },
  "homepage": "https://github.com/jkettmann/universal-react-relay-starter-kit",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-plugin-relay": "^1.4.0",
    "babel-plugin-styled-components": "^1.2.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-universal-import": "^1.2.7",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "5",
    "eslint-plugin-react": "^7.0.1",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.1",
    "react-addons-test-utils": "^15.5.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-hot-server-middleware": "^0.1.0",
    "write-file-webpack-plugin": "^4.1.0"
  },
  "dependencies": {
    "accept-language": "^3.0.17",
    "amazon-cognito-identity-js": "^1.21.0",
    "amazon-cognito-js": "^1.1.0",
    "aws-sdk": "^2.127.0",
    "classnames": "^2.2.5",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.2",
    "cors": "^2.8.4",
    "debug": "^3.0.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "express-graphql": "^0.6.6",
    "express-session": "^1.15.3",
    "farce": "^0.2.1",
    "formsy-react": "^0.19.2",
    "found": "^0.3.0",
    "found-relay": "v0.3.0-alpha.7",
    "graphql": "^0.10.1",
    "graphql-errors": "^2.1.0",
    "graphql-relay": "^0.5.2",
    "helmet": "^3.9.0",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.1.4",
    "jsonwebtoken": "^8.0.1",
    "jwk-to-pem": "^1.2.6",
    "lodash": "^4.17.4",
    "multer": "1.3.0",
    "path-to-regexp": "1.7.0",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "next",
    "react-intl": "^2.4.0",
    "react-relay": "^1.4.0",
    "react-s3-uploader": "^4.5.0",
    "react-social-login": "^3.2.1",
    "react-tap-event-plugin": "^2.0.1",
    "react-universal-component": "^2.5.4",
    "recompose": "^0.25.0",
    "relay-compiler": "^1.4.0",
    "relay-runtime": "^1.4.0",
    "sanitize-filename": "^1.6.1",
    "serialize-javascript": "^1.4.0",
    "styled-components": "^2.1.2",
    "webpack": "^3.8.1",
    "webpack-flush-chunks": "^1.1.23"
  }
}
